{
  "plugins": [
    "plugins/markdown",
    "node_modules/better-docs/category"
  ],
  "recurseDepth": 10,
  "source": {
    "include": [
      "app/js",
      "server"
    ],
    "exclude": [
      "node_modules",
      "docs"
    ],
    "includePattern": "\\.js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "better-docs": {
      "name": "TaskBoardAI Documentation",
      "logo": "app/public/favicon.png",
      "title": "TaskBoardAI API Docs",
      "css": "style.css",
      "hideGenerator": true,
      "navLinks": [
        {
          "label": "GitHub",
          "href": "https://github.com/TuckerTucker/TaskBoardAI"
        },
        {
          "label": "README",
          "href": "https://github.com/TuckerTucker/TaskBoardAI/blob/main/README.md"
        }
      ]
    }
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true,
    "idInHeadings": true
  },
  "opts": {
    "template": "node_modules/better-docs",
    "destination": "docs/api",
    "recurse": true,
    "readme": "README.md",
    "tutorials": "tutorials",
    "verbose": true
  }
}